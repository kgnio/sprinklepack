  "tokenColors": [
    {
      "scope": ["support.variable"],
      "settings": {
        "foreground": "#F77F00"
      }
    },
    {
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#FB8500",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.import", "meta.import.ts", "meta.import.js"],
      "settings": {
        "foreground": "#CA6702"
      }
    },
    {
      "scope": ["entity.name.module.js", "entity.name.module.ts"],
      "settings": {
        "foreground": "#3D405B"
      }
    },
    {
      "scope": ["meta.function"],
      "settings": {
        "foreground": "#2A9D8F"
      }
    },
    {
      "scope": ["meta.enum"],
      "settings": {
        "foreground": "#FFB703"
      }
    },
    {
      "scope": ["keyword.control.flow"],
      "settings": {
        "foreground": "#E63946",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        "meta.decorator",
        "meta.decorator punctuation",
        "entity.name.function.decorator"
      ],
      "settings": {
        "foreground": "#CA6702"
      }
    },
    {
      "scope": ["meta.return-type"],
      "settings": {
        "foreground": "#8ECAE6"
      }
    },
    {
      "scope": ["meta.arrow"],
      "settings": {
        "foreground": "#CA6702"
      }
    },
    {
      "scope": ["meta.embedded.block"],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#FFB703",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#2A9D8F",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#E63946",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#F77F00"
      }
    },
    {
      "scope": ["markup.underline"],
      "settings": {
        "foreground": "#219EBC",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["markup.strikethrough"],
      "settings": {
        "foreground": "#E63946",
        "fontStyle": "strikethrough"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#2A9D8F",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.annotation"],
      "settings": {
        "foreground": "#005F73"
      }
    },
    {
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#219EBC"
      }
    },
    {
      "scope": ["keyword.other.unit"],
      "settings": {
        "foreground": "#FFB703"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6C757D", "fontStyle": "italic" }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#2A9D8F" }
    },
    {
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#FFB703", "fontStyle": "bold" }
    },
    {
      "scope": ["constant.language.boolean", "constant.language"],
      "settings": { "foreground": "#FB8500", "fontStyle": "bold" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#CA6702" }
    },
    { "scope": ["keyword.operator"], "settings": { "foreground": "#219EBC" } },
    {
      "scope": ["variable", "identifier", "variable.language"],
      "settings": { "foreground": "#005F73" }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": { "foreground": "#2A9D8F", "fontStyle": "bold" }
    },
    {
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#F77F00" }
    },
    {
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": { "foreground": "#8ECAE6" }
    },
    { "scope": ["punctuation"], "settings": { "foreground": "#495057" } },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FFFFFF", "background": "#E63946" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#FFB703", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.bold"],
      "settings": { "foreground": "#FB8500", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic", "markup.quote"],
      "settings": { "foreground": "#6C757D", "fontStyle": "italic" }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": { "foreground": "#219EBC" }
    },
    {
      "scope": ["support.constant", "constant.other"],
      "settings": { "foreground": "#F77F00" }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": { "foreground": "#CA6702" }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#005F73" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#3D405B" }
    }
  ],
