  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#D26DAA", "fontStyle": "bold" }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#368A9E" }
    },
    {
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#D8A657", "fontStyle": "bold" }
    },
    {
      "scope": ["constant.language.boolean", "constant.language"],
      "settings": { "foreground": "#3D70D6", "fontStyle": "bold" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#8F8BD6" }
    },
    { "scope": ["keyword.operator"], "settings": { "foreground": "#66A4A7" } },
    {
      "scope": ["variable", "identifier"],
      "settings": { "foreground": "#576A9C" }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": { "foreground": "#3D91D6" }
    },
    {
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#9A74C0" }
    },
    {
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": { "foreground": "#368A9E" }
    },
    {
      "scope": ["support.constant", "constant.other"],
      "settings": { "foreground": "#368A9E" }
    },
    {
      "scope": ["punctuation", "punctuation.definition.parameters"],
      "settings": { "foreground": "#4D5B66" }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FFFFFF", "background": "#D95555" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#D8A657", "fontStyle": "bold" }
    },
    { "scope": ["markup.list"], "settings": { "foreground": "#5A90CC" } },
    {
      "scope": ["markup.bold"],
      "settings": { "foreground": "#5A90CC", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic", "markup.quote"],
      "settings": { "foreground": "#7F97AB", "fontStyle": "italic" }
    },
    {
      "scope": ["meta.annotation", "storage.type.annotation"],
      "settings": { "foreground": "#E3DC73" }
    }
  ],
  "semanticTokenColors": {
    "variable": "#576A9C",
    "parameter": "#9A74C0",
    "property": "#368A9E",
    "function": "#3D91D6",
    "method": "#3D91D6",
    "class": "#368A9E",
    "enum": "#66A4A7",
    "interface": "#368A9E",
    "namespace": "#4D5B66",
    "typeParameter": "#8F8BD6",
    "type": "#368A9E",
    "enumMember": "#D8A657",
    "event": "#5A90CC",
    "modifier": "#66A4A7",
    "regexp": "#368A9E",
    "decorator": "#E3DC73",

    "macro": "#C18BD4",
    "label": "#D8A657",
    "comment": {
      "foreground": "#D26DAA",
      "fontStyle": "italic"
    },
    "keyword": {
      "foreground": "#8F8BD6",
      "fontStyle": "bold"
    },
    "string": "#368A9E",
    "number": "#D8A657",
    "boolean": "#3D70D6",
    "operator": "#66A4A7",

    "builtinType": "#368A9E",
    "builtinFunction": "#3D91D6",
    "builtinVariable": "#5A90CC",

    "annotation": "#E3DC73",
    "attribute": "#368A9E",
    "constant": "#368A9E",

    "deprecated": {
      "foreground": "#B0C4D2",
      "fontStyle": "strikethrough"
    },
    "invalid": {
      "foreground": "#FFFFFF",
      "background": "#D95555"
    },

    "symbol.defaultLibrary": "#7CAACB",
    "symbol.local": "#576A9C"
  }
}
