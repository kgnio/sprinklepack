      "scope": ["storage.type"],
      "settings": { "foreground": "#00BFFF", "fontStyle": "bold" }
    },
    {
      "scope": ["storage"],
      "settings": { "foreground": "#00BFFF", "fontStyle": "bold" }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#00FFF7"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#FFD000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["constant.language.boolean", "constant.language"],
      "settings": {
        "foreground": "#FF0080",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#FF00FF"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#FF7E00"
      }
    },
    {
      "scope": ["variable", "identifier", "variable.language"],
      "settings": {
        "foreground": "#FFFF00"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#00FF87"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FF82C8"
      }
    },
    {
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": {
        "foreground": "#00BFFF"
      }
    },
    {
      "scope": ["support.constant", "constant.other"],
      "settings": {
        "foreground": "#FF9800"
      }
    },
    {
      "scope": ["punctuation", "punctuation.definition.parameters"],
      "settings": {
        "foreground": "#FFDD57"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#FF0055"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#FF3C00",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#00E0FF"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#FF00A8",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic", "markup.quote"],
      "settings": {
        "foreground": "#BB99FF",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#FFDD57"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#00E0FF"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#FF9900"
      }
    }
