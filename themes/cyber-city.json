  "tokenColors": [
    { "scope": ["constant.other"], "settings": { "foreground": "#FF9800" } },
    {
      "scope": ["constant.character.escape"],
      "settings": { "foreground": "#FF3C00" }
    },
    {
      "scope": ["constant.language.boolean"],
      "settings": { "foreground": "#FF0080", "fontStyle": "bold" }
    },
    { "scope": ["keyword.operator"], "settings": { "foreground": "#FF00A8" } },
    { "scope": ["variable.language"], "settings": { "foreground": "#00FFF7" } },
