    { "scope": ["meta.decorator"], "settings": { "foreground": "#E9950C" } },
    { "scope": ["meta.jsx.children"], "settings": { "foreground": "#00A67D" } },
    {
      "scope": ["entity.other.attribute-name.jsx", "meta.tag.attributes"],
      "settings": { "foreground": "#9CDCFE" }
    },
    {
      "scope": ["support.class.component"],
      "settings": { "foreground": "#569CD6" }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#B5B5B5", "fontStyle": "italic" }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#00A67D" }
    },
    { "scope": ["constant.numeric"], "settings": { "foreground": "#E9950C" } },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#DF3079" }
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "support.function",
        "meta.function-call"
      ],
      "settings": { "foreground": "#E6E6E6" }
    },
    {
      "scope": ["variable", "identifier", "meta.definition.variable"],
      "settings": { "foreground": "#E9E9E9" }
    },
    {
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#E9E9E9" }
    },
    {
      "scope": ["invalid"],
      "settings": { "foreground": "#FFFFFF", "background": "#F44747" }
    },
    {
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#569CD6", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.bold"],
      "settings": { "fontStyle": "bold", "foreground": "#D7BA7D" }
    },
    {
      "scope": ["markup.italic"],
      "settings": { "fontStyle": "italic", "foreground": "#D7BA7D" }
    },
    { "scope": ["markup.underline"], "settings": { "fontStyle": "underline" } },
    {
      "scope": ["markup.quote"],
      "settings": { "foreground": "#CE9178", "fontStyle": "italic" }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": { "foreground": "#9CDCFE" }
    },
    {
      "scope": ["meta.tag", "punctuation.definition.tag"],
      "settings": { "foreground": "#569CD6" }
    }
