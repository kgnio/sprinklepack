  "tokenColors": [
    {
      "scope": ["constant.language.boolean", "constant.language"],
      "settings": { "foreground": "#E9950C", "fontStyle": "bold" }
    },
    { "scope": ["markup.list"], "settings": { "foreground": "#D7BA7D" } },
    {
      "scope": ["meta.object-literal.key"],
      "settings": { "foreground": "#9CDCFE" }
    },
    { "scope": ["entity.name.tag"], "settings": { "foreground": "#569CD6" } },
    { "scope": ["meta.function"], "settings": { "foreground": "#E6E6E6" } },
    {
      "scope": ["entity.name.module.ts"],
      "settings": { "foreground": "#9CDCFE" }
    },
    {
      "scope": ["meta.import", "meta.import.ts", "meta.import.js"],
      "settings": { "foreground": "#DF3079" }
    },
    {
      "scope": ["keyword.control.flow"],
      "settings": { "foreground": "#F44747", "fontStyle": "bold" }
    },
    {
      "scope": ["meta.decorator punctuation", "entity.name.function.decorator"],
      "settings": { "foreground": "#E9950C" }
    },
    { "scope": ["meta.enum"], "settings": { "foreground": "#B5CEA8" } },
    { "scope": ["keyword.return"], "settings": { "foreground": "#DF3079" } },
