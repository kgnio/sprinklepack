  "tokenColors": [
    {
      "scope": ["meta.function.parameters", "meta.function.parameter"],
      "settings": {
        "foreground": "#5A9367"
      }
    },
    {
      "scope": ["meta.decorator", "meta.decorator punctuation"],
      "settings": {
        "foreground": "#9A348E"
      }
    },
    {
      "scope": [
        "meta.import",
        "meta.import keyword",
        "meta.import punctuation"
      ],
      "settings": {
        "foreground": "#0077B6"
      }
    },
    {
      "scope": ["meta.class", "meta.class body", "meta.class.identifier"],
      "settings": {
        "foreground": "#3A86FF"
      }
    },
    {
      "scope": [
        "meta.interface",
        "meta.interface body",
        "meta.interface.identifier"
      ],
      "settings": {
        "foreground": "#2D9BAF"
      }
    },
    {
      "scope": ["entity.name.module.js", "entity.name.module.ts"],
      "settings": {
        "foreground": "#F3722C"
      }
    },
    {
      "scope": ["support.variable", "support.other.variable"],
      "settings": {
        "foreground": "#3A4D41"
      }
    },
    {
      "scope": ["meta.embedded"],
      "settings": {
        "foreground": "#E76F51"
      }
    },
    {
      "scope": ["meta.tag.js", "meta.tag.tsx"],
      "settings": {
        "foreground": "#E07A5F"
      }
    },
    {
      "scope": ["meta.jsx.children.tsx", "meta.jsx.children.js"],
      "settings": {
        "foreground": "#FF6700"
      }
    },
    {
      "scope": [
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control"
      ],
      "settings": {
        "foreground": "#B93326",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["keyword.control.as", "keyword.control.from"],
      "settings": {
        "foreground": "#0077B6"
      }
    },
    {
      "scope": ["meta.attribute", "meta.attribute.name"],
      "settings": {
        "foreground": "#0081A7"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#9D4EDD"
      }
    },
    {
      "scope": ["meta.return-type", "meta.return-type punctuation"],
      "settings": {
        "foreground": "#2D9BAF"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6E7D7C", "fontStyle": "italic" }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": { "foreground": "#3CB371" }
    },
    {
      "scope": ["constant.numeric"],
      "settings": { "foreground": "#E76F51", "fontStyle": "bold" }
    },
    {
      "scope": ["constant.language.boolean", "constant.language"],
      "settings": { "foreground": "#0077B6", "fontStyle": "bold" }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#457B9D" }
    },
    {
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#E63946" }
    },
    {
      "scope": ["variable", "identifier", "variable.language"],
      "settings": { "foreground": "#264653" }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": { "foreground": "#5A9367" }
    },
    {
      "scope": ["variable.parameter"],
      "settings": { "foreground": "#9D4EDD" }
    },
    {
      "scope": ["entity.name.type", "support.type", "entity.name.class"],
      "settings": { "foreground": "#3A86FF" }
    },
    {
      "scope": ["support.constant", "constant.other"],
      "settings": { "foreground": "#FB8500" }
    },
    {
      "scope": ["punctuation", "punctuation.definition.parameters"],
      "settings": { "foreground": "#5C6F68" }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FFFFFF", "background": "#B93326" }
    },
    {
      "scope": ["markup.heading"],
      "settings": { "foreground": "#B77729", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.list"],
      "settings": { "foreground": "#577590" }
    },
    {
      "scope": ["markup.bold"],
      "settings": { "foreground": "#F94144", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic", "markup.quote"],
      "settings": { "foreground": "#7F9EAC", "fontStyle": "italic" }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": { "foreground": "#FFB703" }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#E07A5F" }
    },
    {
      "scope": ["support.class.component"],
      "settings": { "foreground": "#6A0572" }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": { "foreground": "#F3722C" }
    },
    {
      "scope": ["meta.jsx.children"],
      "settings": { "foreground": "#FF6700" }
    },
    {
      "scope": ["meta.tag.attributes", "entity.other.attribute-name.jsx"],
      "settings": { "foreground": "#0081A7" }
    },
    {
      "scope": ["variable.other.readwrite.js", "variable.other.readwrite.ts"],
      "settings": { "foreground": "#6D597A" }
    },
    {
      "scope": ["storage.type.function.arrow"],
      "settings": { "foreground": "#9A348E" }
    },
    {
      "scope": ["meta.brace.round", "meta.brace.square", "meta.brace.curly"],
      "settings": { "foreground": "#3D405B" }
    }
